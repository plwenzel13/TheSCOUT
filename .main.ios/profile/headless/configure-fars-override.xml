<project name="configure-fars-override">
<!-- Should be generated by the IDE -->
<target name="configure" depends="configure-override-projects, configure-override-classpaths">
    <echo message="override configure" />
</target>

<target name="configure-override-classpaths">
    <fileset id="oracle.eclipse.tools.maf.libraries"
        dir="${adf.rt.zip.root}/classpath-container-libs" includes="**/*.jar" />
</target>

<target name="configure-override-projects">
               <property name="oepe.adfmf.assembly.project.dir.0" value="/Users/patriciawenzel/Documents/workspace/TheSCOUT" />
           <property name="oepe.adfmf.assembly.project.dir.1" value="/Users/patriciawenzel/Documents/workspace/TheSCOUTApplication" />
           <property name="oepe.adfmf.assembly.project.dir.2" value="/Users/patriciawenzel/Documents/workspace/TheSCOUTView" />
        
    <!-- To support user libraries or rest framework libraries in project class path, add property in following format
    <property name="<ConstantValue>.<UserLibName>.0" value="<Absolute path to library file>" />
    To support User Library
    Change the <ConstantValue> to oepe.maf.classpath.userLibrary
    Change the <UserLibName> to the actual user library name for the project
    Change the <Absolute path to library file> to the value to the absolute path to library
    
    To support Rest Framework libraries
    Change the <ConstantValue> to restLibs
    Change the <UserLibName> to oracle.eclipse.tools.maf.project.rest.classpath.v21
    Change the <Absolute path to library file> to the value to the absolute path to library
    
    If more than 1 library is present in the user library or rest classpath container then add subsequent property by incrementing 0 as 1, 2 and so on. -->
    
           	
     	       	<property name="$propName" value="$propValue" />
    
	<!-- To support user libraries or rest framework libraries in project class path, add propertyset in following format
    <propertyset id="<ContantValue>.<UserLibName>.jarset">
        <propertyref prefix="<ContantValue>.<UserLibName>"/>
    </propertyset>
    To support User Library
    Change the <ConstantValue> to oepe.maf.classpath.userLibrary
    Change the <UserLibName> to the actual user library name for the project
    
    To support Rest Framework libraries
    Change the <ConstantValue> to restLibs
    Change the <UserLibName> to oracle.eclipse.tools.maf.project.rest.classpath.v21 -->

			<propertyset id="[].jarset">
       		 <propertyref prefix="[]"/>
    	</propertyset>
   	    	 
	
    <propertyset id="oepe.adfmf.eclipse-projects">
        <propertyref prefix="oepe.adfmf.assembly.project.dir"/>
    </propertyset>

    
    <propertyset id="oepe.adfmf.libraries">
        <propertyref prefix="oepe.adfmf.library"/>
    </propertyset>
</target>
</project>
