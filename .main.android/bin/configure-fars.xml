<project name="configure-fars">
	<!-- Should be generated by the IDE -->
	<target name="configure">
		<!-- oepe.adfmf.eclipse-projects is used by the default far builder to know
			what set of directory root paths constitute the projects included in the application -->
		<propertyset id="oepe.adfmf.eclipse-projects">
			<propertyref prefix="oepe.adfmf.assembly.project.dir"/>
		</propertyset>

		<!-- oepe.adfmf.libraries is used by the default far builder to know what set of libraries
			need to be copied into each project's FAR -->
		<propertyset id="oepe.adfmf.libraries">
			<propertyref prefix="oepe.adfmf.library"/>
		</propertyset>

		<echo message="Original configure" />
	</target>
</project>